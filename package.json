{
  "name": "meng-ui-react",
  "version": "0.1.0",
  "private": false,
  "description": "A Component Library for React",
  "homepage": "http://qianxun.shop/meng-ui-react",
  "keywords": [
    "meng-ui",
    "component library",
    "ui framework",
    "ui",
    "react"
  ],
  "license": "MIT",
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "async-validator": "^4.2.5",
    "axios": "^0.27.2",
    "babel-plugin-macros": "^3.1.0",
    "classnames": "^2.3.1",
    "lodash-es": "^4.17.21",
    "react-transition-group": "^4.4.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "build": "npm run clean && npm run build-es && npm run build-umd",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-es": "rollup --config rollup/rollup.esm.config.js",
    "build-umd": "rollup --config rollup/rollup.umd.config.js",
    "build-css": "sass ./src/styles/index.scss ./dist/index.css",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install",
    "prepublish": "npm run test:nowatch && npm run lint && npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qianxunyimeng/meng-ui-react.git"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "additional-rule": "off",
      "testing-library/no-render-in-setup": [
        "warn",
        {
          "allowTestingFrameworkSetupHook": "beforeAll"
        }
      ],
      "testing-library/no-node-access": [
        "warn",
        {
          "allowContainerFirstChild": true
        }
      ],
      "testing-library/prefer-screen-queries": [
        "warn"
      ],
      "testing-library/prefer-presence-queries": [
        "warn"
      ],
      "testing-library/no-container": "warn",
      "testing-library/render-result-naming-convention": "warn",
      "testing-library/no-wait-for-multiple-assertions": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "additional-typescript-only-rule": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.5.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.22",
    "rollup-plugin-sass": "^1.2.13",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "sass": "^1.54.4",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "webpack": "^5.72.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ]
  }
}
